// Start with the base Swedish layout
partial alphanumeric_keys
xkb_symbols "basic" {
    key <AD11> { [         aring, Aring, braceright, braceleft ] };
    key <AE00> { [       plus, question, bracketright, bracketleft ] };
    key <BKSL> { [      dead_acute, grave, less, greater, bar, asciitilde ] };
};
