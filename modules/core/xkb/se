default partial alphanumeric_keys
xkb_symbols "custom" {
    include "se(basic)"
    name[Group1]= "Swedish [Custom]";

    // --- Key Remapping ---
    key <AE12> { [    less,    greater,      tilde,    questionexclam ] }; // Key left of Backspace
    key <BKSL> { [ apostrophe,  asterisk,        bar,       dead_acute ] }; // Key below Backspace
    key <AD11> { [ bracketleft, braceleft,       aring,           Aring ] }; // 'å' key
    key <AD12> { [ bracketright, braceright, dead_diaeresis, dead_circumflex ] }; // '¨' key

    include "level3(ralt_switch)"
};
